{
  "name": "node-koa2",
  "version": "0.1.0",
  "author": "lidengkai <lidengkai9487@163.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run watch",
    "watch": "nodemon src/main.ts --config nodemon.config.json",
    "prod": "cross-env NODE_ENV=production npm run pm2",
    "dev": "cross-env NODE_ENV=development npm run pm2",
    "test": "cross-env NODE_ENV=testing npm run pm2",
    "clean": "find ./public -not -name .gitkeep -exec rm {} \\;",
    "pm2": "pm2 delete pm2.config.json && pm2 start pm2.config.json",
    "stop": "pm2 delete pm2.config.json",
    "tsc": "tsc --watch"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "crypto": "~1.0.1",
    "formidable": "~1.2.2",
    "koa": "~2.13.1",
    "koa-bodyparser": "~4.3.0",
    "koa-json": "~2.0.2",
    "koa-json-error": "~3.1.2",
    "koa-logger": "~3.2.1",
    "koa-router": "~10.0.0",
    "koa-session": "~6.2.0",
    "koa-static-server": "~1.5.2",
    "koa2-cors": "~2.0.6",
    "moment": "~2.29.1",
    "mysql2": "~2.3.0",
    "sequelize": "~6.6.5"
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/plugin-proposal-decorators": "~7.14.5",
    "@babel/plugin-transform-runtime": "~7.15.0",
    "@babel/preset-env": "~7.15.0",
    "@babel/preset-typescript": "~7.15.0",
    "@types/formidable": "~1.2.3",
    "@types/koa": "~2.13.4",
    "@types/koa-bodyparser": "~4.3.3",
    "@types/koa-json": "~2.0.20",
    "@types/koa-json-error": "~3.1.4",
    "@types/koa-logger": "~3.1.1",
    "@types/koa-router": "~7.4.4",
    "@types/koa-session": "~5.10.4",
    "@types/koa-static": "~4.0.2",
    "@types/koa-static-server": "~1.5.0",
    "@types/koa2-cors": "~2.0.2",
    "@types/module-alias": "~2.0.1",
    "cross-env": "~7.0.3",
    "nodemon": "~2.0.12",
    "pm2": "~5.1.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2"
  }
}
